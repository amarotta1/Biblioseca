<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
  assembly="Biblioseca.Model"
  namespace="Biblioseca.Model">
	<class name="Penalty" table="Penaltys">
		<id name="Id">
			<generator class="identity" />
		</id>
		<many-to-one name="loan">
			<column name="LoanID" sql-type ="int" not-null="true" />
		</many-to-one>
		<property name="initialDate">
			<column name="initialDate" sql-type ="date" not-null="true" />
		</property>
		<property name="finishDate">
			<column name="finishDate" sql-type ="date" not-null="true" />
		</property>		
	</class>
	<!--Recordar ir a propiedades del archivo y seleccionar que sea un recurso incrustado-->
</hibernate-mapping>
